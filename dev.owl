    <owl:ObjectProperty rdf:about="http://kerndatensatz-forschung.de/version1/technisches_datenmodell/owl/Basis#hatBefristung">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://kerndatensatz-forschung.de/version1/technisches_datenmodell/owl/Basis#Beschaeftigung"/>
        <rdfs:range rdf:resource="http://kerndatensatz-forschung.de/version1/technisches_datenmodell/owl/Basis#Befristung"/>
